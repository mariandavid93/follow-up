<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.iquest.model.Institution" table="INSTITUTION">
        <cache usage="read-write"/>
        <id column="ID" name="id" type="integer"/>
        <property column="name" name="name" type="string"/>
        <property column="address" name="address" type="string"/>
        <property column="owner" name="owner" type="string"/>
        <property column="email" name="email" type="string"/>
        <property column="phone" name="phone" type="string"/>
        <property column="accountNo" name="accountNo" type="string"/>
        <property column="service" name="service" type="string"/>
    </class>
</hibernate-mapping>
